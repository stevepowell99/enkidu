# Local development example (DO NOT commit real secrets here)

# Single-user auth (required) - sent as: Authorization: Bearer <token>
ENKIDU_ADMIN_TOKEN=

# Supabase (required) - Project Settings -> API
SUPABASE_URL=
# Use the Supabase "secret" server key here (or legacy service_role if you use that)
SUPABASE_SERVICE_ROLE_KEY=
# Optional (only needed for sql_select tool): direct Postgres connection string (read-only usage enforced in code)
# You can get this from Supabase Project Settings -> Database -> Connection string.
SUPABASE_DB_URL=

# Gemini (Google AI Studio) (required)
GEMINI_API_KEY=
# Optional
GEMINI_MODEL=gemini-3-flash-preview
GEMINI_EMBED_MODEL=text-embedding-004

# Cloud Run / cross-origin UI hosting
# If your UI is hosted elsewhere (e.g. Netlify), set the allowed origin for CORS:
ENKIDU_CORS_ORIGIN=


